net: "examples/bone-finetune/train_val_bone_modify.prototxt"
test_iter: 15
test_interval: 100
base_lr: 0.001
lr_policy: "step"
gamma: 0.1
# stepsize should also be lower, as we're closer to being done
stepsize: 10000
display: 100
max_iter: 25000
momentum: 0.9
weight_decay: 0.0005
snapshot_prefix: "bone_modify";
# uncomment the following to default to CPU mode solving